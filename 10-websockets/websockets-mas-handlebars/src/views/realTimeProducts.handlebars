<div id="products">
    <div class="row row-cols-1 row-cols-md-3 g-4">
        {{#each products}}
        <div class="col">
            <div class="card">
                <img src="{{this.thumbnail}}" class="card-img-top" alt="Hollywood Sign on The Hill" />

                <div class="card-body">
                    <h5 class="card-title">{{this.title}}</h5>
                    <p class="card-text">
                        {{this.description}}
                    </p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/{{static}}/js/index.js"></script>